@model IReadOnlyCollection<BookModel>

@{
    ViewData["Title"] = "Result search";
}

<h1>Result search</h1>

@if (Model.Count == 0)
{
    <p>Nothing search.</p>
}
else
{
    <ul>
        @foreach (var book in Model)
        {
            <li><a href="@Url.Action("Index","Book",new { id = book.Id })">@book.Title</a></li>
        }
    </ul>
}
